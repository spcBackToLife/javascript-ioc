{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "project": "./tsconfig.json"
    },
    "extends": [
      "xigua"
    ],
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "no-unused-vars": 0,
      "react/react-in-jsx-scope": 0,
      "@typescript-eslint/no-unused-vars": 2,
      "jsx-a11y/no-noninteractive-element-interactions": 0
    }
  }
  